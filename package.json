{
  "name": "nodejs-ts-mysql-nestjs-jest-vite-vitest-reactjs-docker_remaining-students-tracker",
  "version": "1.0.1",
  "description": "Tracking of remaining students",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compose:up:build": "docker compose up --build",
    "compose:up": "docker compose up",
    "compose:down:rmop": "docker compose down --remove-orphans",
    "compose:down": "docker compose down",
    "build": "MARIADB_DATABASE=production npm run compose:up:build",
    "start": "MARIADB_DATABASE=production npm run compose:up",
    "build:dev": "MARIADB_DATABASE=development npm run compose:up:build",
    "start:dev": "MARIADB_DATABASE=development npm run compose:up",
    "stop": "npm run compose:down",
    "stop:rm": "npm run compose:down:rmop"
  },
  "author": "Rodrigo G. Oliveira",
  "license": "MIT",
  "devDependencies": {
    "dotenv": "^16.4.5",
    "puppeteer": "^23.6.0"
  }
}
